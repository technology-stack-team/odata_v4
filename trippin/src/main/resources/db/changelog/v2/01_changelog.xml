<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <changeSet id="v2-01-01" author="rahuljain">
        <createTable tableName="Event" schemaName="Trippin">
            <column name="PlanItemId" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="DTYPE" type="VARCHAR(250)"/>
            <column name="ConfirmationCode" type="VARCHAR(250)">
                <constraints nullable="false"/>
            </column>
            <column name="StartsAt" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="EndsAt" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="Duration" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
